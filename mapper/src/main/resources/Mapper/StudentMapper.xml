<mapper namespace="com.csg.mapper.StudentMapper">

<select id="selectStudentCount" resultType="java.lang.Integer">
	select count(*) from student
</select>
<select id="selectStudents" resultType="com.csg.model.Student">
	select * from student
</select>

</mapper>